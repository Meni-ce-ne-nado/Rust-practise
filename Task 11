fn is_palindrome(x: u32) -> bool {
    let str_x = x.to_string();        
    let reversed_str = str_x.chars().rev().collect::<String>(); 
    str_x == reversed_str            
}

fn main() {
    println!("{}", is_palindrome(123));   
    println!("{}", is_palindrome(121));   
    println!("{}", is_palindrome(1221));  
}
